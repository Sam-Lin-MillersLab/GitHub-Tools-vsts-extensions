{
  "manifestVersion": 1,
  "id": "githubtools",
  "version": "0.1.44",
  "name": "GitHub Tools",
  "scopes": [],
  "description": "GitHub Tools for VSTS build and Release",
  "publisher": "DevMikaelKrief",
  "galleryFlags": [
    "Preview"
  ],
  "icons": {
    "default": "static/images/logo.png"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "demands": [
  ],
  "tags": [
    "GitHub","Tag","Release"
  ],
  "categories": [
    "Build and release"
  ],
  "content": {
    "license": {
      "path": "license.md"
    },
    "details": {
      "path": "overview.md"
    }
  },
  "links": {
    "getstarted": {
      "uri": "https://github.com/mikaelkrief/GitHub-Tools-vsts-extensions"
    },
    "support": {
      "uri": "https://github.com/mikaelkrief/GitHub-Tools-vsts-extensions"
    },
    "issues": {
      "uri": "https://github.com/mikaelkrief/GitHub-Tools-vsts-extensions"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/mikaelkrief/GitHub-Tools-vsts-extensions"
  },
  "branding": {
    "color": "rgb(220, 235, 252)",
    "theme": "light"
  },
  "files": [
    {
      "path": "GitHubTag"
    },
    {
      "path": "GitHubRelease"
    },
    {
      "path": "static",
      "addressable": true
    }
  ],
  "contributions": [
    {
      "id": "GitHubTag",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "type": "ms.vss-distributed-task.task",
      "properties": {
        "name": "GitHubTag"
      }
    },
    {
      "id": "GitHubRelease",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "type": "ms.vss-distributed-task.task",
      "properties": {
        "name": "GitHubRelease"
      }
    }
  ]
}